<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="mycook.dell.example.com.patientapp">

    <application
        android:name=".config.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="AA_DB_NAME"
            android:value="PatientsDbOne"/>
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1"/>
       <!-- <meta-data
            android:name="AA_MODEL"
            android:value="mycook.dell.example.com.patientapp.model.Patient"/>-->

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ListPatientsActivity">
        </activity>
        <activity android:name=".search_patients">
        </activity>
    </application>

</manifest>